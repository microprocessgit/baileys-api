<%- include('cabecalho'); %>
  <div class="row">
    <div class="col-8">
      <h3 class="text-secondary m-5">Cadastro de Clientes</h3>
    </div>
    <div class="col-4 textEnd">
      <a href="cadastrados" class="link m-4 link-success">Clientes Cadstrados</a>
    </div>
  </div>
  <div class="d-flex justify-content-center">
    <div class="col-5">
      <form class="borda" method="POST" action="/app/client/">
        <%- include('mensagem'); %>
        <div class="mb-3">
          <label class="form-label">Cliente</label>
          <input type="text" class="form-control" id="client" name="client" aria-describedby="" required>
        </div>
        <div class="mb-3">
          <label class="form-label">Chave</label>
          <input type="text" class="form-control" id="key" name="key" aria-describedby="" required>
        </div>
        <div class="mb-3">
          <label class="form-label">Webhook</label>
          <input type="text" class="form-control" id="webhook" name="webhook" aria-describedby="">
        </div>
        <button type="submit" class="btn btn-primary">Gravar</button>
      </form>
    </div>
  </div>
  <%- include('rodape'); %>